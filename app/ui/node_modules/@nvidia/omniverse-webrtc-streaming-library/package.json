{
    "name": "@nvidia/omniverse-webrtc-streaming-library",
    "private": false,
    "version": "5.4.4",
    "description": "WebRTC streaming library for omniverse cloud streaming use cases",
    "author": "Nvidia",
    "license": "SEE LICENSE IN LICENSE.txt",
    "keywords": [
        "omniverse",
        "webrtc",
        "streaming",
        "cloud"
    ],
    "type": "module",
    "repository": {
        "type": "git",
        "url": "https://gitlab-master.nvidia.com/omniverse/saas/web-streaming-client"
    },
    "publishConfig": {
        "@nvidia:registry": "https://urm.nvidia.com/artifactory/api/npm/sw-pandora-npm-local/",
        "access": "public",
        "main": "./dist/index.js"
    },
    "exports": {
        ".": {
            "import": "./dist/omniverse-webrtc-streaming-library.js",
            "require": "./dist/omniverse-webrtc-streaming-library.umd.cjs"
        }
    },
    "main": "./dist/omniverse-webrtc-streaming-library.umd.cjs",
    "module": "./dist/omniverse-webrtc-streaming-library.js",
    "types": "./dist/omniverse-webrtc-streaming-library.d.ts",
    "files": [
        "dist",
        "docs",
        "README.md",
        "LICENSE.txt"
    ],
    "engines": {
        "node": ">=19.0.0",
        "npm": ">=10.0.0"
    },
    "scripts": {
        "minify": "uglifyjs-folder dist -e -x .js -o dist",
        "dev": "vite",
        "build:json2ts": "npx json2ts -i src/resources/schema/**/*.schema.json -o src/resources/types/ --style.singleQuote",
        "build": "tsc -b && vite build && npm run minify",
        "build:dev": "tsc -b && vite build --mode='dev'",
        "build:docs": "npx typedoc src/index.ts",
        "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
        "preview": "vite preview",
        "test": "cross-env BUILD_ENV=test vitest"
    },
    "dependencies": {
        "deepcopy": "^2.1.0",
        "deepmerge": "^4.3.1",
        "is-plain-object": "^5.0.0"
    },
    "devDependencies": {
        "@aws-sdk/client-cognito-identity": "3.2.0",
        "@aws-sdk/client-s3": "3.2.0",
        "@aws-sdk/credential-provider-cognito-identity": "3.2.0",
        "@aws-sdk/types": "3.0.0",
        "@gamestream/ragnarok": "0.71.80",
        "@testing-library/jest-dom": "^6.6.3",
        "@types/jsonschema": "^1.1.1",
        "@types/mocha": "^10.0.10",
        "@types/node": "^20.14.11",
        "@types/offscreencanvas": "2019.7.0",
        "@typescript-eslint/eslint-plugin": "^7.15.0",
        "@typescript-eslint/parser": "^7.15.0",
        "chai": "^4.3.7",
        "cross-env": "^7.0.3",
        "eslint": "^8.57.0",
        "jsdom": "^26.0.0",
        "json-schema-to-typescript": "^15.0.4",
        "jsonschema": "^1.5.0",
        "lightstep-tracer": "0.28.0",
        "mocha": "^10.2.0",
        "opentracing": "0.14.5",
        "path": "^0.12.7",
        "ts-node": "^10.9.2",
        "typedoc": "^0.26.4",
        "typescript": "^5.2.2",
        "uglifyjs-folder": "^3.3.0",
        "url": "^0.11.3",
        "vite": "^5.3.4",
        "vite-plugin-dts": "^4.2.1",
        "vitest": "^3.1.1"
    },
    "directories": {
        "doc": "docs"
    }
}